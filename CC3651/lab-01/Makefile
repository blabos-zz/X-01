first: all
####### Implicit rules

.SUFFIXES: .o .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"

####### Build rules

all: $(TARGET)

$(TARGET):  $(OBJECTS)  
	@$(CHK_DIR_EXISTS) bin/ || $(MKDIR) bin/  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJCOMP) $(LIBS)


dist: 
	@$(CHK_DIR_EXISTS) tmp/obj/wesley-222080061_lab-01 || $(MKDIR) tmp/obj/wesley-222080061_lab-01 
	$(COPY_FILE) --parents $(SOURCES) $(DIST) tmp/obj/wesley-222080061_lab-01/ && $(COPY_FILE) --parents src/contacts.h src/menu.h tmp/obj/wesley-222080061_lab-01/ && $(COPY_FILE) --parents src/contacts.c src/main.c src/menu.c tmp/obj/wesley-222080061_lab-01/ && (cd `dirname tmp/obj/wesley-222080061_lab-01` && $(TAR) wesley-222080061_lab-01.tar wesley-222080061_lab-01 && $(COMPRESS) wesley-222080061_lab-01.tar) && $(MOVE) `dirname tmp/obj/wesley-222080061_lab-01`/wesley-222080061_lab-01.tar.gz . && $(DEL_FILE) -r tmp/obj/wesley-222080061_lab-01


clean:compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) 
	-$(DEL_FILE) wesley-222080061_lab-01.tar.gz
	-$(DEL_FILE) -r tmp
	-$(DEL_FILE) -r .tmp

compiler_clean: 

####### Compile

tmp/obj/contacts.o: src/contacts.c src/contacts.h
	@$(CHK_DIR_EXISTS) $(OBJECTS_DIR) || $(MKDIR) $(OBJECTS_DIR)
	$(CC) -c $(CFLAGS) $(INCPATH) -o tmp/obj/contacts.o src/contacts.c

tmp/obj/main.o: src/main.c src/contacts.h \
		src/menu.h
	@$(CHK_DIR_EXISTS) $(OBJECTS_DIR) || $(MKDIR) $(OBJECTS_DIR)
	$(CC) -c $(CFLAGS) $(INCPATH) -o tmp/obj/main.o src/main.c

tmp/obj/menu.o: src/menu.c src/menu.h \
		src/contacts.h
	@$(CHK_DIR_EXISTS) $(OBJECTS_DIR) || $(MKDIR) $(OBJECTS_DIR)
	$(CC) -c $(CFLAGS) $(INCPATH) -o tmp/obj/menu.o src/menu.c

